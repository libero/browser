{
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Libero\\Browser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\Libero\\Browser\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "symfony/browser-kit": "^4.1",
        "symfony/debug": "^4.1"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*"
    },
    "config": {
        "platform": {
            "php": "7.2.8"
        },
        "sort-packages": true
    }
}
