{
    "name": "libero/browser",
    "description": "Serve content read from a Libero API to the public",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Libero\\Browser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\Libero\\Browser\\": "tests/",
            "tests\\Libero\\ContentPageBundle\\": "vendor-extra/ContentPageBundle/tests/",
            "tests\\Libero\\JatsContentBundle\\": "vendor-extra/JatsContentBundle/tests/",
            "tests\\Libero\\LiberoContentBundle\\": "vendor-extra/LiberoContentBundle/tests/",
            "tests\\Libero\\ViewsBundle\\": "vendor-extra/ViewsBundle/tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "csa/guzzle-bundle": "^3.1",
        "libero/content-page-bundle": "^1.0@dev",
        "libero/jats-content-bundle": "^1.0@dev",
        "libero/libero-content-bundle": "^1.0@dev",
        "libero/patterns-bundle": "^1.0@dev",
        "libero/views-bundle": "^1.0@dev",
        "symfony/asset": "^4.1",
        "symfony/config": "^4.1",
        "symfony/console": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/http-foundation": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/monolog-bundle": "^3.3",
        "symfony/routing": "^4.1",
        "symfony/translation": "^4.2",
        "symfony/twig-bundle": "^4.1",
        "symfony/yaml": "^4.1",
        "twig/twig": "^2.7"
    },
    "require-dev": {
        "csa/guzzle-cache-middleware": "^1.0",
        "libero/coding-standard": "^0.3",
        "php-vfs/php-vfs": "^1.4",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpstan/phpstan-symfony": "^0.11",
        "phpunit/phpunit": "^7.4",
        "psr/http-message": "^1.0",
        "symfony/browser-kit": "^4.1",
        "symfony/css-selector": "^4.1",
        "symfony/debug": "^4.1",
        "symfony/dom-crawler": "^4.1",
        "symfony/filesystem": "^4.1",
        "symfony/web-profiler-bundle": "^4.1"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./vendor-extra/*"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
