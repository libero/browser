<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <defaults public="false"/>

        <service id="Twig\Extensions\IntlExtension" class="Twig\Extensions\IntlExtension">
            <tag name="twig.extension"/>
        </service>

        <service id="Libero\LiberoPatternsBundle\EventListener\BuildView\DateLangListener"
            class="Libero\LiberoPatternsBundle\EventListener\BuildView\DateLangListener">
            <tag name="kernel.event_listener" event="libero.view.build" method="onBuildView"/>
        </service>

    </services>

</container>
