{
    "name": "libero/patterns-bundle",
    "description": "Libero patterns",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Libero\\LiberoPatternsBundle\\": "src/"
        },
        "files": [
            "src/constants.php"
        ]
    },
    "require": {
        "php": "^7.2",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.7"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
