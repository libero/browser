{
    "name": "libero/jats-content-bundle",
    "description": "JATS content handler",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Libero\\JatsContentBundle\\": "src/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-dom": "*",
        "fluentdom/fluentdom": "^7.0",
        "libero/page-bundle": "^1.0@dev",
        "libero/views-bundle": "^1.0@dev",
        "symfony/config": "^4.1",
        "symfony/contracts": "^1.0",
        "symfony/dependency-injection": "^4.1",
        "symfony/event-dispatcher": "^4.1",
        "symfony/http-kernel": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
